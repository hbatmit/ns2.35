	LDFLAGS="$LDFLAGS -Wl,-export-dynamic"
	if test "$have_dl" = yes; then
	    SHLIB_LD="${CC} -shared"
	    DL_LIBS="-ldl"
	    DL_LD_FLAGS="-rdynamic"
	    DL_LD_SEARCH_FLAGS=""
	else
	    ac_fn_c_check_header_mongrel "$LINENO" "dld.h" "ac_cv_header_dld_h" "$ac_includes_default"
if test "x$ac_cv_header_dld_h" = xyes; then :

		SHLIB_LD="ld -shared"
		DL_LIBS="-ldld"
		DL_LD_FLAGS=""
		DL_LD_SEARCH_FLAGS=""
