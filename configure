# Step 3: set configuration options based on system name and version.

case $system in
    AIX-*)
	enable_dl="no"
	;;
    BSD/OS-2.1*|BSD/OS-3*)
	SHLIB_CFLAGS=""
	SHLIB_LD="shlicc -r"
